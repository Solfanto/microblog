<main class="panel">
  <h1>Search among latest posts</h1>
  <form action="<%= search_path %>" method="get" accept-charset="UTF-8" role="search">
    <div class="form-group">
      <input name="q" class="form-control" type="search" placeholder="" aria-label="Search" aria-describedby="search-button-addon" value="<%= params[:q] %>" autofocus="true">
    </div>
    <div class="form-group d-flex justify-content-between">
      &nbsp;
      <button class="btn btn-primary" id="search-button-addon" type="submit">Search</button>
    </div>
  </form>
</main>
<aside class="panel">
  <h1>Trending</h1>
</aside>